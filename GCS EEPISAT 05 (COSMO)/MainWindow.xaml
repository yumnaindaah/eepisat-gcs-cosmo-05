                TEAM ID
                        </TextBlock>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="Regular" FontSize="15" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Top">
                2000
                        </TextBlock>
                    </Grid>

                    <!--State-->
                    <Image Grid.Column="2" Source="/Assets/Vector_State.png" Height="35" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Grid Grid.Column="3">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.5*"></RowDefinition>
                            <RowDefinition Height="0.5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="Bold" FontSize="15" Grid.Column="3" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Bottom">
                STATE
                        </TextBlock>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="Regular" FontSize="14" Grid.Column="3" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Top">
                MISSION_READY
                        </TextBlock>
                    </Grid>

                    <!--Mission Time-->
                    <Image Grid.Column="4" Source="/Assets/gcs_time.png" Height="35" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Grid Grid.Column="5">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.5*"></RowDefinition>
                            <RowDefinition Height="0.5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="Bold" FontSize="14" Grid.Column="5" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Bottom">
                MISSION_TIME
                        </TextBlock>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="Regular" FontSize="15" Grid.Column="5" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Top">
                00:00:00
                        </TextBlock>
                    </Grid>

                    <!--Satelit-->
                    <Image Grid.Column="6" Source="/Assets/Vector_satelit.png" Height="35" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Grid Grid.Column="7">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.5*"></RowDefinition>
                            <RowDefinition Height="0.5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="Bold" FontSize="15" Grid.Column="7" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Bottom">
                SATELLITES
                        </TextBlock>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="Regular" FontSize="15" Grid.Column="7" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Top">
                12 (GOOD)
                        </TextBlock>
                    </Grid>

                    <!--baterai-->
                    <Image Grid.Column="8" Source="/Assets/battery.png" Height="35" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Grid Grid.Column="9">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.5*"></RowDefinition>
                            <RowDefinition Height="0.5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="Bold" FontSize="15" Grid.Column="9" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Bottom">
                BATTERY
                        </TextBlock>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="Regular" FontSize="15" Grid.Column="9" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Top">
                100% / 8.4 v
                        </TextBlock>
                    </Grid>


                    <!--Air speed-->
                    <Image Grid.Column="10" Source="/Assets/Vector_Air.png" Height="35" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Grid Grid.Column="11">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.5*"></RowDefinition>
                            <RowDefinition Height="0.5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="Bold" FontSize="15" Grid.Column="11" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Bottom">
                AIR SPEED
                        </TextBlock>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="Regular" FontSize="15" Grid.Column="11" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Top">
                10.0 m/s
                        </TextBlock>

                    </Grid>
                </Grid>
            </Border>

            <!-- GCS Com -->
            <Border Grid.Column="2" CornerRadius="10" Background="#FAFAFA" Height="50">
                <Border.Effect>
                    <DropShadowEffect Color="Gray" Direction="320" ShadowDepth="5" BlurRadius="10" Opacity="0.5"/>
                </Border.Effect>
                <Grid>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="0.2*"></ColumnDefinition>
                        <ColumnDefinition Width="0.5*"></ColumnDefinition>
                        <ColumnDefinition Width="0.15*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>
                    <Image Grid.Column="0" Source="/Assets/Vector_COM.png" Height="30" Width="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Grid Grid.Column="1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="0.5*"></RowDefinition>
                            <RowDefinition Height="0.5*"></RowDefinition>
                        </Grid.RowDefinitions>
                        <TextBlock Foreground="#292929" FontFamily="Inter" FontWeight="SemiBold" FontSize="14" Grid.Row="0" HorizontalAlignment="Left" VerticalAlignment="Bottom" Margin="6,0,0,0">
                GCS COM
                        </TextBlock>
                        <Label Foreground="#777575" FontFamily="Inter" FontWeight="Regular" FontSize="14" Grid.Column="1" Grid.Row="1" HorizontalAlignment="Left" VerticalAlignment="Bottom" Content="Disconnected"/>
                    </Grid>

                    <Border Grid.Column="2" CornerRadius="50" Background="#FF6157" Height="20" Width="20"  HorizontalAlignment="Center" VerticalAlignment="Center"/>
                </Grid>
            </Border>
            <!-- COM Connector -->
            <Border Grid.Column="3" CornerRadius="10" Background="#FAFAFA" Height="50" Width="45" HorizontalAlignment="Right" VerticalAlignment="Center">
                <Border.Effect>
                    <DropShadowEffect Color="Gray" Direction="320" ShadowDepth="5" BlurRadius="10" Opacity="0.5"/>
                </Border.Effect>
                <Image Grid.Column="0" Source="/Assets/connected.png" Margin="0,5,0,5"/>
            </Border>

            <!-- COM Refresh -->
            <Border Grid.Column="4" CornerRadius="10" Background="#FAFAFA" Height="50" HorizontalAlignment="Right" VerticalAlignment="Center">
                <Border.Effect>
                    <DropShadowEffect Color="Gray" Direction="320" ShadowDepth="5" BlurRadius="10" Opacity="0.5"/>
                </Border.Effect>
                <Image Grid.Column="0" Source="/Assets/refresh.png" Margin="6,8,6,7"/>
            </Border>

            <!-- COMPORT -->
            <Border Grid.Column="5" CornerRadius="10" Background="#FAFAFA" Height="50" HorizontalAlignment="Right" VerticalAlignment="Center">
                <Border.Effect>
                    <DropShadowEffect Color="Gray" Direction="320" ShadowDepth="5" BlurRadius="10" Opacity="0.5"/>
                </Border.Effect>
                <Label Foreground="#292929" FontFamily="Inter" FontWeight="Bold" FontSize="20" Content="COMPORT" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Border>

            <!-- BAUDRATE -->
            <Border Grid.Column="6" CornerRadius="10" Background="#FAFAFA" Height="50" HorizontalAlignment="Right" VerticalAlignment="Center">
                <Border.Effect>
                    <DropShadowEffect Color="Gray" Direction="320" ShadowDepth="5" BlurRadius="10" Opacity="0.5"/>
                </Border.Effect>
                <Label Foreground="#292929" FontFamily="Inter" FontWeight="Bold" FontSize="20" Content="BAUDRATE" HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Border>
        </Grid>
