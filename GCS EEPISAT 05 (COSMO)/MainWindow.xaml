        <!--HEADER BAWAH-->
        <Grid Grid.Row="1" Margin="0,60,0,0" Grid.RowSpan="2">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"></ColumnDefinition>
                <ColumnDefinition Width="0.32*"></ColumnDefinition>
                <ColumnDefinition Width="0.15*"></ColumnDefinition>
                <ColumnDefinition Width="0.33*"></ColumnDefinition>
                <ColumnDefinition Width="0.15*"></ColumnDefinition>
                <ColumnDefinition Width="0.28*"></ColumnDefinition>
                <ColumnDefinition Width="0.15*"></ColumnDefinition>
                <ColumnDefinition Width="0.28*"></ColumnDefinition>
                <ColumnDefinition Width="0.15*"></ColumnDefinition>
                <ColumnDefinition Width="0.38*"></ColumnDefinition>
                <ColumnDefinition Width="1*"></ColumnDefinition>
            </Grid.ColumnDefinitions>

            <RadioButton x:Name="HomeMenu" Click="HomeNavClick" Style="{DynamicResource HomeMenu}" IsChecked="True" Grid.Column="1" Margin="1,1,1,1"/>
            <RadioButton x:Name="GraphMenu" Click="GraphNavClick" Style="{DynamicResource GraphMenu}" Grid.Column="3" Margin="1,1,1,1"/>
            <RadioButton x:Name="MapMenu" Click="MapNavClick" Style="{DynamicResource MapMenu}"  Grid.Column="5" Margin="1,1,1,1"/>
            <RadioButton x:Name="DataMenu" Click="DataNavClick" Style="{DynamicResource DataMenu}"  Grid.Column="7" Margin="1,1,1,1"/>
            <RadioButton x:Name="SettingMenu" Click="SettingNavClick" Style="{DynamicResource SettingMenu}"  Grid.Column="9" Margin="1,1,1,1"/>
        </Grid>

        <!--ELEMEN-->
        <Border Grid.Row="2" CornerRadius="0" Background="Transparent" Margin="0,55,0,65" Grid.RowSpan="4">
            <TabControl x:Name="MainPage" Background="Transparent" BorderBrush="Transparent" ScrollViewer.CanContentScroll="True" SelectionChanged="MainPage_SelectionChanged">
                <!--<TabControl.ItemContainerStyle>
                    <Style TargetType="{x:Type TabItem}">
                        <Setter Property="Visibility" Value="Collapsed"/>
                    </Style>
                </TabControl.ItemContainerStyle>-->
