        Height="{x:Static SystemParameters.PrimaryScreenHeight}">
    <Grid>
        <Grid.RowDefinitions>
            <!--header atas-->
            <RowDefinition Height="85*" />
            <!--header tengah-->
            <RowDefinition Height="60.235" />
            <!--header bawah-->
            <RowDefinition Height="55*" />
            <!--element-->
            <RowDefinition Height="256*" />
            <RowDefinition Height="247*"/>
